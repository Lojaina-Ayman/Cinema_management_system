CREATE TABLE Cinema (
    Name VARCHAR(255) PRIMARY KEY,
    Branches VARCHAR(255) NOT NULL,
    Location VARCHAR(255) NOT NULL,
    Licenses VARCHAR(255),
    OpeningHours NUMBER(12) NOT NULL 
);

