CREATE TABLE Customer
(
	Customer_ID Number(10) PRIMARY KEY ,
	Age Number(2) ,
	Name Number(100) ,
	Phone_no Number(11) ,
	Email Varchar2(100) ,
	Gender Varchar2(10) 
);

